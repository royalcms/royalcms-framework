{
    "name": "royalcms/mail",
    "description": "The Royalcms Mail package.",
    "license": "MIT",
    "homepage": "https://royalcms.cn",
    "support": {
        "issues": "https://github.com/royalcms/framework/issues",
        "source": "https://github.com/royalcms/framework"
    },
    "authors": [
        {
            "name": "Royal Wang",
            "email": "hxroyal@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "erusev/parsedown": "~1.6",
        "royalcms/container": "5.0.*",
        "royalcms/contracts": "5.0.*",
        "royalcms/support": "5.0.*",
        "psr/log": "~1.0",
        "swiftmailer/swiftmailer": "~5.4",
        "tijsverkoyen/css-to-inline-styles": "~2.2"
    },
    "autoload": {
        "psr-4": {
            "Royalcms\\Component\\Mail\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "5.0-dev"
        }
    },
    "suggest": {
        "aws/aws-sdk-php": "Required to use the SES mail driver (~3.0).",
        "guzzlehttp/guzzle": "Required to use the Mailgun and Mandrill mail drivers (~6.0)."
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
